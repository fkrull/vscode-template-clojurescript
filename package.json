{
    "name": "vscode-extension-clojurescript",
    "version": "0.0.1",
    "publisher": "fkrull",
    "engines": {
        "vscode": "^1.16.0"
    },
    "main": "./extension",
    "activationEvents": [
        "onCommand:hellocljs.helloWorld"
    ],
    "contributes": {
        "commands": [{
            "command": "hellocljs.helloWorld",
            "title": "Hello World",
            "category": "Hello ClojureScript"
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "lumo build.cljs"
    },
    "devDependencies": {
        "lumo-cljs": "^1.8.0-beta",
        "vscode": "^1.1.5"
    }
}
